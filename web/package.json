{
    "name": "web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode prod",
        "dev:sta": "vite --mode sta",
        "dev:dev": "vite --mode dev",
        "build": "vue-tsc --noEmit && vite build",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:sta": "vue-tsc --noEmit && vite build --mode sta",
        "build:dev": "vue-tsc --noEmit && vite build --mode dev",
        "revision": "echo $GITHUB_SHA > ./public/revision",
        "preview": "vite preview",
        "prettier": "prettier --write .",
        "generate": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@auth0/auth0-vue": "^1.0.2",
        "@headlessui/vue": "^1.7.4",
        "@heroicons/vue": "^2.0.13",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/showdown": "^2.0.0",
        "@urql/exchange-auth": "^1.0.0",
        "@urql/vue": "^1.0.2",
        "bccm-video-player": "^1.2.4",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "rudder-sdk-js": "^2.20.0",
        "showdown": "^2.1.0",
        "swiper": "^8.4.5",
        "vue": "^3.2.45",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@graphql-codegen/cli": "^2.14.1",
        "@graphql-codegen/typescript": "^2.8.2",
        "@graphql-codegen/typescript-operations": "^2.5.7",
        "@graphql-codegen/typescript-vue-urql": "^2.3.6",
        "@types/node": "^18.11.9",
        "@vitejs/plugin-vue": "^3.2.0",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier-vue": "^4.2.0",
        "eslint-plugin-vue": "^9.8.0",
        "postcss": "^8.4.19",
        "prettier": "^2.8.0",
        "tailwindcss": "^3.2.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "vite": "^3.2.4",
        "vue-tsc": "^1.0.9"
    }
}
